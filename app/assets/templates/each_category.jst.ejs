<div class="row">
  <div class="col-md-12">
    <h4><%= category %></h4>
  </div>
</div>

<% _(values).each(function(el) { %>
  <% if(el.baseMSRP != 0) { %>
    <div class"row group">
      <div class="each-feature">
        <div class="col-md-1">
          <input data-name='<%= el.name %>'
          data-description="<%= el.description %>"
          data-baseMSRP="<%= el.baseMSRP %>"
          type="checkbox">
        </div>

        <div class="col-md-6">
          <%= el.name %>
        </div>

        <div class="col-md-5">
          $<%= el.baseMSRP %>
        </div>
      </div>
    </div>
    <br>
  <% } %>
<% }) %>
