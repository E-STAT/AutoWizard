<div id="<%= styleId %>" class="carousel slide" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators">


    <% _(allPics.attributes).each(function(pic, index) { %>
      <% if (index === "0") { %>
        <li data-target="#<%= styleId %>" data-slide-to="<%= index %>" class="active"></li>
      <% } else { %>
        <li data-target="#<%= styleId %>" data-slide-to="<%= index %>"></li>
      <% } %>
    <% }) %>

  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner">
    <% _(allPics.attributes).each(function(pic, index) { %>
      <% if (index === "0") { %>
        <div class="item active">
      <% } else { %>
        <div class="item">
      <% } %>
        <img src="http://media.ed.edmunds-media.com/<%= pic.id.slice(10) %>_815.jpg">
      </div>
    <% }) %>
  </div>

  <!-- Controls -->
  <a class="left carousel-control" data-target="#<%= styleId %>" role="button" data-slide="prev">
    <span class="glyphicon glyphicon-chevron-left"></span>
  </a>
  <a class="right carousel-control" data-target="#<%= styleId %>" role="button" data-slide="next">
    <span class="glyphicon glyphicon-chevron-right"></span>
  </a>
</div> <!-- Carousel -->
<button type="button" class="btn btn-default btn-lg delete">
  Delete Car<span class="glyphicon glyphicon-ax" aria-hidden="true"></span>
</button>
