<button type="button" class="btn btn-default btn-lg">
  Garage  <span class="glyphicon glyphicon-cars" aria-hidden="true"></span>
</button>

<h1 class="all-makes">Select A Make</h1>

<ul class="makes-models-trims">
  <% makes.each(function(make) { %>
    <li class="list-group-item" data-id="<%= make.id %>" data-name="<%= make.attributes.name %>">
      <a><%= make.attributes.name %></a>
    </li>
  <% }) %>
</ul>
