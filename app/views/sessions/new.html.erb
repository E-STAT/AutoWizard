<!-- Warming Up -->
<link href='http://fonts.googleapis.com/css?family=Buenard:700' rel='stylesheet' type='text/css'>
<script src="http://pupunzi.com/mb.components/mb.YTPlayer/demo/inc/jquery.mb.YTPlayer.js"></script>

<script>
jQuery(function(){
  jQuery(".player").mb_YTPlayer();
});
</script>

<!--Video Section-->
<div class="content-section video-section">
  <div class="pattern-overlay">
    <a id="bgndVideo" class="player" data-property="{videoURL:'https://www.youtube.com/watch?v=SlG-ualRTvU',containment:'.video-section', quality:'small', autoPlay:true, mute:true, opacity:1}">bg</a>
    <div class="container">
      <div class="row">
        <div class="col-lg-12">

          <h1>AutoWizard</h1>
          <h3>Sign In to Continue</h3>

            <form action="/session" method="post">
              <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
              <div class="form-group">
                <label>Username<br>
                  <input name="user[username]" type="text"><br>
                </label>
              </div>

              <div class="form-group">
                <label>Password<br>
                  <input name="user[password]" type="password"><br>
                </label>
              </div>

              <input type="submit" value="Submit" class="btn btn-primary"><br>
            </form>

        </div>
      </div>
    </div>
  </div>
</div>
<!--Video Section Ends Here-->
